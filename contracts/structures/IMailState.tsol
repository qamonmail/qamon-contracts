pragma ever-solidity ^0.62.0;

interface IMailState{

    struct MailUser {
        address addr;
        uint256 pubkey;
    }

    struct PendingMail {
        MailUser user;
        address mail;
        bytes meta;
        address sender;
        uint16 metaVersion;
        address send_gas_to;
    }

    enum MailBoxType {
        Inbox,
        Outbox
    }
}
