pragma ever-solidity ^0.62.0;


interface IMailAccount {
    function saveInMailAddress(uint32 nonce, address mail, bytes meta, address sender, uint16 metaVersion) external;
    function saveOutMailAddress(uint32 nonce, address mail, bytes meta, uint16 metaVersion) external;

    function upgrade(
        TvmCell _code,
        uint32 _newVersion,
        address _remainingGasTo
    ) external;
}
